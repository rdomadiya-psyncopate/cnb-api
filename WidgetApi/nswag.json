{
  "runtime": "Net80",
  "defaultVariables": null,
  "documentGenerator": {
    "fromDocument": {
      "url": "../openapi.v1.yaml",
      "output": null
    }
  },
  "codeGenerators": {
    "openApiToCSharpController": {
      "className": "{controller}",
      "namespace": "WidgetApi.Generated",
      "generateModelValidationAttributes": true,
      "controllerStyle": "Abstract",
      "useActionResultType": true,
      "generateOptionalParameters": true,
      "output": "Generated/Controllers/GeneratedControllers.cs"
    },
    "openApiToCSharpClient": {
      "className": "Models",
      "namespace": "WidgetApi.Generated.Models",
      "generateDtoTypes": true,
      "output": "Generated/Models/GeneratedModels.cs"
    }
  }
}
